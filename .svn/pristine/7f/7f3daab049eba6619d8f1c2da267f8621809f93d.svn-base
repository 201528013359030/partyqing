.wrap{ width:100%; margin:0 auto; width:640px\9;  height: 100%; }
.moGrid{ width:90%; margin:0 auto; overflow:hidden; clear:both; padding-bottom:.8em;}
.moBox{ width:100%; background-color:#fcfcfc;}
.moTit{padding:1.125em 0; text-align: center;}

.mg_top{margin-top:20px;}

ul.listset{width:100%; margin-bottom:1em;}
.listset li{border-bottom:#f2f2f2 1px solid; position:relative;}
.listset li .setpic{width:70px; height:70px; overflow: hidden; display: inline-block;vertical-align: top;}
.listset li .setpic .ficon{width:60px; height:60px; line-height: 60px; font-size: 24px; text-align: center; color: #fff; border-radius: 4px; -webkit-border-radius: 4px;}
.listset li .setInfo{ display: inline-block;vertical-align: top; width:70%; }
.listset li .setInfo .name{ font-size:18px; line-height:30px; white-space: nowrap;}
.listset li .setInfo .type{font-size:.875em; color:#a0a0a0; line-height: 1.25; margin:0; margin-top:6px;}

.listset .setIteam{display: block;  padding:.75em;}
.listset .setIteam .tip{ position:absolute; width:30px; height:40px; top:50%; right:0; color:#dedede; /* font-size:40px;*/ overflow: hidden;
margin-top:-20px;
font-size:20px;
line-height: 40px;
}

.stateBox{ width:100%; background:#fff; position:relative; padding-top:1.25em; padding-bottom:.4em; margin-bottom:.75em; min-height:6em ;}
.stateBox .title{height:1.5em; line-height: 1.5; margin-bottom:.8em;text-align: center; font-weight: normal;}
.stateBox .title .ficon{width:1.5em;height:1.5em; line-height: 1.5; background:#6FBA75;color:#fff; 
font-size:.875em;
-webkit-border-radius:50px;
border-radius:50px;
-webkit-transform: translateY(-1px);
transform: translateY(-1px);
}
.stateBox .titleOnly{margin-top:0.75em;}

.controlBox{width:100%; margin-top:1.25em;}
.controlBox .lead{color:#9b9b9b;}
.controlBox .btn{display: block; width:100%; height:3.25em; line-height:3.25; color:#fff; display: inline-block; vertical-align: top;
-webkit-border-radius: 5px;
border-radius: 5px; background:#51A3EF; text-align: center;}

.chartwrap{width:100%; overflow: hidden; margin-top:20px;}
#chartBox{/*width:50%;*/ height:430px;}
#chartBox2{/*width:50%;*/ height:430px;}

