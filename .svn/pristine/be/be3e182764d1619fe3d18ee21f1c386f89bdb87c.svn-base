	background: url("../img/pull-icon@2x.png") no-repeat 0px 0px / 40px 80px; width: 40px; height: 40px; float: left; display: block; -webkit-background-size: 40px 80px; -webkit-transition-property: -webkit-transform; -webkit-transition-duration: 250ms;
}
#pullDown .pullDownIcon {
	-webkit-transform: rotate(0deg) translateZ(0) scale(.5);
}
#pullUp .pullUpIcon {
	-webkit-transform: rotate(-180deg) translateZ(0) scale(.5);
}
.flip#pullDown .pullDownIcon {
	-webkit-transform: rotate(-180deg) translateZ(0) scale(.5);
}
.flip#pullUp .pullUpIcon {
	-webkit-transform: rotate(0deg) translateZ(0) scale(.5);
}
.loading#pullDown .pullDownIcon {
	background-position: 0px 100%; -webkit-transform: rotate(0deg) translateZ(0) scale(.5); -webkit-transition-duration: 0ms; -webkit-animation-name: loading; -webkit-animation-duration: 2s; -webkit-animation-iteration-count: infinite; -webkit-animation-timing-function: linear;
}
.loading#pullUp .pullUpIcon {
	background-position: 0px 100%; -webkit-transform: rotate(0deg) translateZ(0) scale(.5); -webkit-transition-duration: 0ms; -webkit-animation-name: loading; -webkit-animation-duration: 2s; -webkit-animation-iteration-count: infinite; -webkit-animation-timing-function: linear;
}
