  body{margin:0;padding:0;font-family:"Microsoft YaHei";font-size:16px; background-color:#f5f5f5}
  h1,h2,h3,h4,h5,ol,ul,li,dl,dt,dd,img,p,input{margin:0;padding:0;border:0;}
  a{color:#333;text-decoration:none;}
  *{list-style:none;}
    .top1{background:url("../img/top_bg.png") no-repeat center center;
	       width:100%;
		   height:84px;
		   position:relative;
		   display:block;
		   margin:0 auto;
		   padding:0;
		   z-index:10;
		   -moz-background-size:cover;
		   -webkit-background-size:cover;
		   background-size:cover; 
		   }	
	 #divcss5{position:relative;display:block;padding-left:32px;padding-top:10px;}
     #divcss5 img{width:60px;height:60px; border-radius:50%}
	.border_circle{position:absolute;display:inline-block;top:6px;left:28px;width:68px;height:68px; background-color:#83d9ae; border-radius:34px;z-index:-1;}
	.text_big{position:absolute;top:.5em;left:25%;font-size:1.2em;color:#fff;}
	.text_l{position:absolute;top:2.5em;left:55%;color:#fff;font-size:1em;}
	 .wrap{position:relative;display:block;background:url("../img/tree.png") repeat-y;margin:0 auto;padding:0;width:100%;width:640px\9;height:100%;}
    .listIteam{position:relative;display:block;width:100%;height:auto;}
	.listIteam:nth-child(odd){background:url("../img/leaf_r.png") no-repeat;}
    .listIteam:nth-child(even){background:url("../img/leaf_l.png") no-repeat;}
    .white_k{margin-left:55px; background-color:#fff; width:80%;border-radius:5px;}
	span{position:relative;display:inline-block;font-size:16px; font-color:#333;margin-left:20px;margin-top:8px;}
	
	.img-max{max-width:65%;margin-bottom:10px;}
	  @media screen and (min-width:640px){
     .wrap{width:640px;}
	 
      }
      @media screen and(max-width:640px){
